{
  "name": "marketpulseai",
  "version": "0.1.0",
  "private": true,
  "description": "Tooling and automation for the MarketPulseAI browser extension.",
  "scripts": {
    "lint": "eslint extension --ext .js",
    "test": "node --test",
    "build": "node scripts/build-extension.js",
    "lint:test:build": "npm run lint && npm run test && npm run build",
    "format": "prettier --write .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{json,css,md,html}": "prettier --write"
  },
  "devDependencies": {
    "dotenv": "^17.2.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3"
  },
  "dependencies": {
    "dexie": "^3.2.4"
  }
}
